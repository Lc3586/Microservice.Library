    <PackageReference Include="Senparc.Weixin.MP" Version="16.10.502" />
    <PackageReference Include="Senparc.Weixin.TenPay" Version="1.5.502.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Container\Library.Container.csproj" />
  </ItemGroup>

