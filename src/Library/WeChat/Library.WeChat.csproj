    <PackageReference Include="Microsoft.Extensions.Options" Version="6.0.0" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.9.0" />
    <PackageReference Include="Senparc.CO2NET" Version="2.1.0" />
    <PackageReference Include="Senparc.CO2NET.APM" Version="1.2.0" />
    <PackageReference Include="Senparc.NeuChar" Version="2.1.1.1" />
    <PackageReference Include="Senparc.Weixin" Version="6.15.1" />
    <PackageReference Include="Senparc.Weixin.MP" Version="16.18.2.1" />
    <PackageReference Include="Senparc.Weixin.TenPay" Version="1.12.1" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Annotations\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Http\Library.Http.csproj" />
  </ItemGroup>

</Project>
