<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Library.SuperSocket</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.2-alpha</Version>
    <Authors>LCTR</Authors>
    <Product>LCTR</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Supersocket推展
黑名单
JT协议</Description>
    <PackageProjectUrl>https://github.com/Lc3586/Microservice/tree/master/src/Library/SuperSocket</PackageProjectUrl>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>Library.SuperSocket.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <DocumentationFile>Library.SuperSocket.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="JTProtocol\JTProtocol-template.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="JTProtocol\JTProtocol-template.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Test\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Flee" Version="1.2.2" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.1.3" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="3.1.3" />
    <PackageReference Include="SuperSocket.Client" Version="2.0.0-preview3-06" />
    <PackageReference Include="SuperSocket.SessionContainer" Version="2.0.0-preview3-06" />
    <PackageReference Include="SuperSocket.WebSocket.Server" Version="2.0.0-preview3-06" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Cache\Library.Cache.csproj" />
    <ProjectReference Include="..\Configuration\Library.Configuration.csproj" />
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties /></VisualStudio></ProjectExtensions>

</Project>
