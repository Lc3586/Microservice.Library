<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Library.Models</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.2.0.10-alpha</Version>
    <Authors>LCTR</Authors>
    <Company>LCTR</Company>
    <Product>LCTR</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>类库基础模型</Description>
    <PackageProjectUrl>https://github.com/Lc3586/Microservice/tree/master/src/Library/LibraryModels</PackageProjectUrl>
