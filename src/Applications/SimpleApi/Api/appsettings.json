{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SystemConfig": {
    "AdminId": null,
    "AdminAccount": "admin",
    "AdminInitPassword": "666666",

    "ProjectName": "示例系统",
    "PublishRootUrl": "http://a.com",
    "localRootUrl": "http://localhost:5000",
    //"RunMode": "Publish_Swagger",
    "RunMode": "LocalTest_Strict",

    "JWTSecret": "test123456",

    "WorkerId": 1000,

    //日志配置
    "DefaultLoggerType": "File",

    //缓存配置
    "EnableCache": true,
    "DefaultCacheType": "SystemCache",

    //自定义注入类
    "FxAssembly": [ "Business" ],

    //Swagger接口文档
    "EnableSwagger": true,

    //数据映射配置
    "EnableAutoMapper": true,
    "AutoMapperAssemblys": [ "Model" ],

    //数据库配置
    "MultiDatabases": false,

    //FreeSql
    "EnableFreeSql": true,

    //Elasticsearch
    "EnableElasticsearch": false,

    //Kafka
    "EnableKafka": false,

    //微信服务
    "EnableWeChatService": true,

    //简易身份验证服务
    "EnableSampleAuthentication": true,

    //CAS
    "EnableCAS": false
  }
}
